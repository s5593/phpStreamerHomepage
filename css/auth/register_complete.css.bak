body {
    background-color: #000;
    color: #eee;
    font-family: 'Noto Sans KR', sans-serif;
    margin: 0;
    padding: 0;
  }
  
  .complete-container {
    max-width: 500px;
    margin: 120px auto;
    background-color: #111;
    padding: 40px 30px;
    border-radius: 10px;
    text-align: center;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.7);
  }
  
  .complete-container h2 {
    font-size: 22px;
    margin-bottom: 20px;
    color: #fff;
  }
  
  .complete-container p {
    font-size: 15px;
    margin: 8px 0;
    color: #ccc;
  }
  
  .btn-purple {
    display: inline-block;
    margin-top: 30px;
    background-color: #6a0dad;
    color: #fff;
    text-decoration: none;
    padding: 12px 20px;
    font-weight: bold;
    font-size: 15px;
    border-radius: 8px;
    transition: background-color 0.2s ease;
  }
  
  .btn-purple:hover {
    background-color: #5a0ca5;
  }
  