.login-log-table {
    width: 100%;
    border-collapse: collapse;
}
.login-log-table th, .login-log-table td {
    padding: 8px;
    border: 1px solid #ccc;
    text-align: center;
}
.login-log-table th {
    background-color: #f8f8f8;
}
.btn {
    padding: 6px 12px;
    margin: 4px;
    text-decoration: none;
    border: 1px solid #333;
    display: inline-block;
}
